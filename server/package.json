{
  "name": "talker-backend",
  "version": "1.0.0",
  "description": "Can't afford therapy? Need someone to talk to? You have come to the right place. Talk with an AI Therapist to figure out your daily issues.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "nodemon src/server.js",
    "build": "echo 'Nothing to build for plain JavaScript'",
    "watch": "nodemon src/server.js",
    "seed": "node seed.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "exec": "node src/server.js"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "0.1.37",
    "mongoose": "^8.8.2",
    "openai": "^5.12.2"
  },
  "devDependencies": {
    "@types/mongoose": "^5.11.96",
    "@types/node": "20.12.7",
    "nodemon": "3.1.0"
  }
}
